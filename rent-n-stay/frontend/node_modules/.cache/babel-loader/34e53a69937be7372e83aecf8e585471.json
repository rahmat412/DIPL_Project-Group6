 </label>\r\n                <select \r\n                  v-model=\"uplaceid\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                >\r\n                  <option\r\n                    v-for=\"place in places\" :key=\"place.placeID\"\r\n                    v-bind:value=\"place.placeID\"\r\n                  >\r\n                    {{place.placeID}} - {{ place.placeName }}\r\n                  </option>\r\n                </select>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Password\r\n                </label>\r\n                <input\r\n                  type=\"password\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Password\"\r\n                  v-model=\"upassword\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Phone Number\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Phone Number\"\r\n                  v-model=\"uphone\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"text-center mt-6\">\r\n                <footer class=\"modal-footer\">\r\n                <button\r\n                  @click=\"addOrder\"\r\n                  class=\"bg-emerald-800 text-white active:bg-emerald-600 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1 w-full ease-linear transition-all duration-150\"\r\n                  type=\"button\"\r\n                >\r\n                  Add\r\n                </button>\r\n                </footer>\r\n              </div>\r\n            </form>\r\n        </section>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {  \r\n  data() {\r\n    return {\r\n      un: \"\",\r\n      uemail: \"\",\r\n      upassword: \"\",\r\n      uphone: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    this.getPlaces();\r\n  },\r\n  methods: {\r\n    // add new order\r\n    async addOrder() {\r\n      try {\r\n        await axios.post(\"http://localhost:5000/order\", {\r\n          orderID: Math.random().toString(36).substring(2),\r\n          orderName: this.uname,\r\n          orderEmail: this.uemail,\r\n          orderPassword: this.upassword,\r\n          orderPhone: this.uphone,\r\n        });\r\n        this.$router.go();\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getPlaces() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5000/place\");\r\n        this.places = response.data;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    close() {\r\n      this.$emit('close');\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n  .modal-backdrop {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.3);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .modal {\r\n    background: #FFFFFF;\r\n    overflow-x: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    border-radius: 5px;\r\n  }\r\n\r\n  .modal-header,\r\n  .modal-footer {\r\n    padding: 20px;\r\n    display: flex;\r\n  }\r\n\r\n  .modal-header {\r\n    position: relative;\r\n    border-bottom: 1px solid #eeeeee;\r\n    color: #4AAE9B;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .modal-footer {\r\n    border-top: 1px solid #eeeeee;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .modal-body {\r\n    position: relative;\r\n    width: 500px;\r\n    padding: 20px 20px;\r\n  }\r\n\r\n  .btn-close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    border: none;\r\n    font-size: 20px;\r\n    padding: 20px 20px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    color: #4AAE9B;\r\n    background: transparent;\r\n  }\r\n\r\n  .modal-fade-enter,\r\n  .modal-fade-leave-to {\r\n    opacity: 0;\r\n  }\r\n\r\n  .modal-fade-enter-active,\r\n  .modal-fade-leave-active {\r\n    transition: opacity .5s ease;\r\n  }\r\n</style>"],"sourceRoot":""},"metadata":{},"sourceType":"module"}