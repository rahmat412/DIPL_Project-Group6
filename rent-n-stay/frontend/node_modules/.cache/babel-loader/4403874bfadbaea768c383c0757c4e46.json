\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                >\r\n                  <option\r\n                    v-for=\"place in places\" :key=\"place.placeID\"\r\n                    v-bind:value=\"place.placeID\"\r\n                  >\r\n                    {{place.placeID}} - {{ place.placeName }}\r\n                  </option>\r\n                </select>\r\n                <span v-if=\"v$.placeid.$error\" class=\"text-sm px-2 text-red-500\">{{v$.placeid.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Street\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Street\"\r\n                  v-model=\"ustreet\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2 mt-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  District\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"District\"\r\n                  v-model=\"udistrict\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Regency\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Regency\"\r\n                  v-model=\"uregency\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Postcode\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Postcode\"\r\n                  v-model=\"upostcode\"\r\n                />\r\n              </div>\r\n\r\n              <div class=\"text-center mt-6\">\r\n                <footer class=\"modal-footer\">\r\n                <button\r\n                  @click=\"editAddress\"\r\n                  class=\"bg-emerald-800 text-white active:bg-emerald-600 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1 w-full ease-linear transition-all duration-150\"\r\n                  type=\"button\"\r\n                >\r\n                  Edit\r\n                </button>\r\n                </footer>\r\n              </div>\r\n            </form>\r\n        </section>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n<script>\r\nimport useValidate from '@vuelidate/core';\r\nimport { required } from '@vuelidate/validators';\r\nimport { reactive, computed } from 'vue';\r\nimport Swal from 'sweetalert2';\r\nimport axios from \"axios\";\r\nexport default {\r\n  setup(){\r\n    const state = reactive ({\r\n      placeid: \"\",\r\n      street: \"\",\r\n      district: \"\",\r\n      regency: \"\",\r\n      postcode: \"\",\r\n    })\r\n    const rules = computed(() => {\r\n      return {\r\n        placeid: { required },\r\n        street: { required },\r\n        district: { required},\r\n        regency: { required },\r\n        postcode: { required },\r\n      }\r\n    })\r\n    const v$ = useValidate(rules,state)\r\n    return {\r\n      state,\r\n      v$,\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      places:{}\r\n    };\r\n  },\r\n  props: ['id'],\r\n  created() {\r\n    this.getAddress();\r\n    this.getPlaces();\r\n  },\r\n  methods: {\r\n    async getAddress() {\r\n      try {\r\n        await axios.get(\r\n          'http://localhost:5000/address/'+this.id\r\n        ).then((response) => {\r\n          this.state.id = response.data.addressID;\r\n          this.state.placeid = response.data.placeID;\r\n          this.state.street = response.data.addressStreet;\r\n          this.state.district = response.data.addressDistrict;\r\n          this.state.regency = response.data.addressRegency;\r\n          this.state.postcode = response.data.addressPostcode;\r\n        })\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getPlaces() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5000/place\");\r\n        this.places = response.data;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async editAddress() {\r\n      try {\r\n        await axios.put('http://localhost:5000/address/'+this.id,\r\n          {\r\n            place_id: this.state.placeid,\r\n            address_street: this.state.street,\r\n            address_district: this.state.district,\r\n            address_regency: this.state.regency,\r\n            address_postcode: this.state.postcode,\r\n          }\r\n        );\r\n        this.$router.go()\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    sAlert: function(ico, tit, txt) {\r\n      Swal.fire({\r\n        icon: ico,\r\n        title: tit,\r\n        text: txt,\r\n        showConfirmButton: false,\r\n        timer: 1500\r\n      });\r\n    },\r\n    close() {\r\n      this.$emit('close');\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n  .modal-backdrop {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.3);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .modal {\r\n    background: #FFFFFF;\r\n    overflow-x: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    border-radius: 5px;\r\n  }\r\n\r\n  .modal-header,\r\n  .modal-footer {\r\n    padding: 20px;\r\n    display: flex;\r\n  }\r\n\r\n  .modal-header {\r\n    position: relative;\r\n    border-bottom: 1px solid #eeeeee;\r\n    color: #4AAE9B;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .modal-footer {\r\n    border-top: 1px solid #eeeeee;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .modal-body {\r\n    position: relative;\r\n    width: 500px;\r\n    padding: 20px 20px;\r\n  }\r\n\r\n  .btn-close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    border: none;\r\n    font-size: 20px;\r\n    padding: 20px 20px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    color: #4AAE9B;\r\n    background: transparent;\r\n  }\r\n\r\n  .modal-fade-enter,\r\n  .modal-fade-leave-to {\r\n    opacity: 0;\r\n  }\r\n\r\n  .modal-fade-enter-active,\r\n  .modal-fade-leave-active {\r\n    transition: opacity .5s ease;\r\n  }\r\n</style>"],"sourceRoot":""},"metadata":{},"sourceType":"module"}