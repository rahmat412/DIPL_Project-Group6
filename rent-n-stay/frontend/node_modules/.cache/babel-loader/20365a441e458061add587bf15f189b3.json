{"ast":null,"code":"export default {\n  data: function data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  methods: {\n    SignIn: function SignIn(e) {\n      var _this = this;\n\n      e.preventDefault();\n\n      if (this.password.length > 0) {\n        this.axios.post('http://localhost:5000/login', {\n          email: this.email,\n          password: this.password\n        }).then(function (response) {\n          var role = response.data.user.role;\n          localStorage.setItem('user', JSON.stringify(response.data.user));\n          localStorage.setItem('jwt', response.data.token);\n\n          if (localStorage.getItem('jwt') != null) {\n            _this.$emit('loggedIn');\n\n            if (_this.$route.params.nextUrl != null) {\n              _this.$router.push(_this.$route.params.nextUrl);\n\n              alert(\"fail\");\n            } else {\n              if (role == \"admin\") {\n                _this.$router.push(\"/admin\");\n              } else {\n                _this.$router.push(\"/\");\n              }\n            }\n          }\n        }).catch(function (error) {\n          console.error(error.response);\n        });\n      }\n    }\n  }\n};","map":{"version":3,"sources":["D:\\Project\\temp_rns\\frontend\\src\\views\\auth\\Login.vue"],"names":[],"mappings":"AA4EA,eAAe;AACb,EAAA,IADa,kBACP;AACJ,WAAO;AACL,MAAA,KAAI,EAAI,EADH;AAEL,MAAA,QAAO,EAAI;AAFN,KAAP;AAID,GANY;AAOb,EAAA,OAAM,EAAI;AACR,IAAA,MADQ,kBACD,CADC,EACC;AAAA;;AACP,MAAA,CAAC,CAAC,cAAF;;AACA,UAAI,KAAK,QAAL,CAAc,MAAd,GAAuB,CAA3B,EAA8B;AAC5B,aAAK,KAAL,CAAW,IAAX,CAAgB,6BAAhB,EAA+C;AAC7C,UAAA,KAAK,EAAE,KAAK,KADiC;AAE7C,UAAA,QAAQ,EAAE,KAAK;AAF8B,SAA/C,EAIC,IAJD,CAIM,UAAA,QAAO,EAAK;AAChB,cAAI,IAAG,GAAI,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,IAA9B;AACA,UAAA,YAAY,CAAC,OAAb,CAAqB,MAArB,EAA4B,IAAI,CAAC,SAAL,CAAe,QAAQ,CAAC,IAAT,CAAc,IAA7B,CAA5B;AACA,UAAA,YAAY,CAAC,OAAb,CAAqB,KAArB,EAA2B,QAAQ,CAAC,IAAT,CAAc,KAAzC;;AACA,cAAI,YAAY,CAAC,OAAb,CAAqB,KAArB,KAA+B,IAAnC,EAAwC;AACtC,