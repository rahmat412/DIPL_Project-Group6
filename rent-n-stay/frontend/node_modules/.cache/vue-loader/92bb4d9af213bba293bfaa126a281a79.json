an>\r\n              </div>\r\n\r\n              <div class=\"text-center mt-6\">\r\n                <footer class=\"modal-footer\">\r\n                <button\r\n                  @click=\"editOrder()\"\r\n                  class=\"bg-emerald-800 text-white active:bg-emerald-600 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1 w-full ease-linear transition-all duration-150\"\r\n                  type=\"button\"\r\n                >\r\n                  Edit\r\n                </button>\r\n                </footer>\r\n              </div>\r\n            </form>\r\n        </section>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n<script>\r\nimport useValidate from '@vuelidate/core';\r\nimport { required } from '@vuelidate/validators';\r\nimport { reactive, computed } from 'vue';\r\nimport Swal from 'sweetalert2';\r\nimport axios from \"axios\";\r\nexport default {\r\n  setup(){\r\n    const state = reactive ({\r\n      id: \"\",\r\n      placeid: \"\",\r\n      clientid: \"\",\r\n      status: \"\",\r\n      range: {\r\n        start: \"\",\r\n        end: \"\",\r\n      },\r\n    })\r\n    const rules = computed(() => {\r\n      return {\r\n        id: { required },\r\n        placeid: { required },\r\n        clientid: { required },\r\n        status: { required },\r\n        range: {\r\n          start: { required },\r\n          end: { required },\r\n        },\r\n      }\r\n    })\r\n    const v$ = useValidate(rules,state)\r\n    return {\r\n      state,\r\n      v$,\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      clients:{},\r\n      places:{},\r\n      stats:['Requested','Accepted','Declined','Cancelled','In Progress','Completed']\r\n    };\r\n  },\r\n  props: ['id'],\r\n  created() {\r\n    this.getOrder();\r\n    this.getClients();\r\n    this.getPlaces();\r\n  },\r\n  methods: {\r\n    getOrder() {\r\n      try {\r\n        axios.get(\r\n          'http://localhost:5000/order/'+this.id\r\n        ).then((response) => {\r\n          this.state.id = response.data.orderID;\r\n          this.state.clientid = response.data.clientID;\r\n          this.state.placeid = response.data.placeID;\r\n          this.state.range.start = response.data.orderCheckIn;\r\n          this.state.range.end = response.data.orderCheckOut;\r\n          this.state.status = response.data.orderStatus;\r\n        })\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getClients() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5000/client\");\r\n        this.clients = response.data;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getPlaces() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5000/place\");\r\n        this.places = response.data;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async editOrder() {\r\n      this.v$.$validate()\r\n      if (this.v$.$error) {\r\n        return\r\n      }\r\n      try {\r\n        await axios.put('http://localhost:5000/order/'+this.id,\r\n          {\r\n            client_id: this.state.clientid,\r\n            place_id: this.state.placeid,\r\n            order_checkin: this.state.range.start,\r\n            order_checkout: this.state.range.end,\r\n            order_status: this.state.status\r\n          }\r\n        );\r\n        this.sAlert(\"success\",\"Yeay..\",\"Editing Order Success\");\r\n        this.close();\r\n      } catch (err) {\r\n        console.log(err);\r\n        this.sAlert(\"error\",\"Oops..\",\"Editing Order Failed\");\r\n      }\r\n    },\r\n    sAlert: function(ico, tit, txt) {\r\n      Swal.fire({\r\n        icon: ico,\r\n        title: tit,\r\n        text: txt,\r\n        showConfirmButton: false,\r\n        timer: 1500\r\n      });\r\n    },\r\n    close() {\r\n      this.$emit('close');\r\n      this.$router.go();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n  .modal-backdrop {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.3);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .modal {\r\n    background: #FFFFFF;\r\n    overflow-x: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    border-radius: 5px;\r\n  }\r\n\r\n  .modal-header,\r\n  .modal-footer {\r\n    padding: 20px;\r\n    display: flex;\r\n  }\r\n\r\n  .modal-header {\r\n    position: relative;\r\n    border-bottom: 1px solid #eeeeee;\r\n    color: #4AAE9B;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .modal-footer {\r\n    border-top: 1px solid #eeeeee;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .modal-body {\r\n    position: relative;\r\n    width: 500px;\r\n    padding: 20px 20px;\r\n  }\r\n\r\n  .btn-close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    border: none;\r\n    font-size: 20px;\r\n    padding: 20px 20px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    color: #4AAE9B;\r\n    background: transparent;\r\n  }\r\n\r\n  .modal-fade-enter,\r\n  .modal-fade-leave-to {\r\n    opacity: 0;\r\n  }\r\n\r\n  .modal-fade-enter-active,\r\n  .modal-fade-leave-active {\r\n    transition: opacity .5s ease;\r\n  }\r\n</style>"]}]}