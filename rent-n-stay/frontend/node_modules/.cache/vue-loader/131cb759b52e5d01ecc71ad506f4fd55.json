hone: \"\",\n    };\n  },\n  components: {\n    Navbar,\n    FooterComponent,\n  },\n  created: function () {\n    this.getProfile();\n  },\n  methods: {\n    // Get Product By Id\n    async getProfile() {\n      try {\n        var role = localStorage.getItem('role');\n        var response;\n        if (role==1){\n          response = await axios.get(\n            \"http://localhost:5000/editadmin/${localStorage.getItem('user')}\"\n          );\n          this.name = response.data.adminName;\n          this.email = response.data.adminEmail;\n          this.password = response.data.adminPassword;\n          this.phone = response.data.adminPhone;\n        } else if (role==2){\n          response = await axios.get(\n            \"http://localhost:5000/editowner/${localStorage.getItem('user')}\"\n          );\n          this.name = response.data.ownerName;\n          this.email = response.data.ownerEmail;\n          this.password = response.data.ownerPassword;\n          this.phone = response.data.ownerPhone;\n        } else if (role==3){\n          response = await axios.get(\n            \"http://localhost:5000/editclient/${localStorage.getItem('user')}\"\n          );\n          this.name = response.data.clientName;\n          this.email = response.data.clientEmail;\n          this.password = response.data.clientPassword;\n          this.phone = response.data.clientPhone;\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    },\n \n    // Update product\n    async updateProfile() {\n      try {\n        await axios.put(\n          \"http://localhost:5000/products/${localStorage.getItem('user')}\",\n          {\n            name: this.name,\n            email: this.email,\n            password: this.password,\n            phone: this.phone,\n          }\n        );\n        this.name = \"\";\n        this.email = \"\";\n        this.password = \"\";\n        this.phone = \"\";\n        this.$router.push(\"/profile\");\n      } catch (err) {\n        console.log(err);\n      }\n    },\n  },\n};\n</script>\n"]}]}