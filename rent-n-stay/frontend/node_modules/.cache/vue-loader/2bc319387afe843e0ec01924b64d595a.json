sContent":["<template>\r\n  <transition name=\"modal-fade\">\r\n    <div class=\"modal-backdrop\">\r\n      <div class=\"modal\">\r\n        <header class=\"modal-header\">\r\n            <h6 class=\"text-emerald-500 text-lg font-bold\">\r\n                Edit Client\r\n            </h6>\r\n          <a\r\n            type=\"button\"\r\n            class=\"btn-close\"\r\n            @click=\"close\"\r\n          >\r\n            <i class=\"fas fa-times\"></i>\r\n          </a>\r\n        </header>\r\n\r\n        <section class=\"modal-body\">\r\n            <form>\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2 mt-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  ID\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"disabled border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"ID\"\r\n                  v-model=\"state.id\"\r\n                  disabled\r\n                />\r\n                <span v-if=\"v$.id.$error\" class=\"text-left block text-sm px-2 text-red-500\">{{v$.id.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2 mt-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Name\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Name\"\r\n                  v-model=\"state.name\"\r\n                />\r\n                <span v-if=\"v$.name.$error\" class=\"text-left block text-sm px-2 text-red-500\">{{v$.name.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2 mt-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Email\r\n                </label>\r\n                <input\r\n                  type=\"email\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Email\"\r\n                  v-model=\"state.email\"\r\n                />\r\n                <span v-if=\"v$.email.$error\" class=\"text-left block text-sm px-2 text-red-500\">{{v$.email.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Password\r\n                </label>\r\n                <input\r\n                  type=\"password\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Password\"\r\n                  v-model=\"state.password\"\r\n                />\r\n                <span v-if=\"v$.password.$error\" class=\"text-left block text-sm px-2 text-red-500\">{{v$.password.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"relative w-full mb-3\">\r\n                <label\r\n                  class=\"text-left block uppercase text-emerald-600 text-xs font-bold mb-2\"\r\n                  htmlFor=\"grid-password\"\r\n                >\r\n                  Phone Number\r\n                </label>\r\n                <input\r\n                  type=\"text\"\r\n                  class=\"border-0 px-3 py-3 placeholder-emerald-300 text-emerald-600 bg-white rounded text-sm shadow focus:outline-none focus:ring w-full ease-linear transition-all duration-150\"\r\n                  placeholder=\"Phone Number\"\r\n                  v-model=\"state.phone\"\r\n                />\r\n                <span v-if=\"v$.phone.$error\" class=\"text-left block text-sm px-2 text-red-500\">{{v$.phone.$errors[0].$message}}</span>\r\n              </div>\r\n\r\n              <div class=\"text-center mt-6\">\r\n                <footer class=\"modal-footer\">\r\n                <button\r\n                  @click=\"editClient()\"\r\n                  class=\"bg-emerald-800 text-white active:bg-emerald-600 text-sm font-bold uppercase px-6 py-3 rounded shadow hover:shadow-lg outline-none focus:outline-none mr-1 mb-1 w-full ease-linear transition-all duration-150\"\r\n                  type=\"button\"\r\n                >\r\n                  Edit\r\n                </button>\r\n                </footer>\r\n              </div>\r\n            </form>\r\n        </section>\r\n      </div>\r\n    </div>\r\n  </transition>\r\n</template>\r\n<script>\r\nimport useValidate from '@vuelidate/core';\r\nimport { required, email, minLength } from '@vuelidate/validators';\r\nimport { reactive, computed } from 'vue';\r\nimport Swal from 'sweetalert2';\r\nimport axios from \"axios\";\r\nexport default {  \r\n  setup(){\r\n    const state = reactive ({\r\n      ie: \"\",\r\n      name: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      phone: \"\",\r\n    })\r\n    const rules = computed(() => {\r\n      return {\r\n        id: { required },\r\n        name: { required },\r\n        email: { required, email },\r\n        password: { required, minLength: minLength(6) },\r\n        phone: { required },\r\n      }\r\n    })\r\n    const v$ = useValidate(rules,state)\r\n    return {\r\n      state,\r\n      v$,\r\n    }\r\n  },\r\n  props: ['id'],\r\n  created() {\r\n    this.getClient();\r\n  },\r\n  methods: {\r\n    async getClient() {\r\n      try {\r\n        await axios.get(\r\n          'http://localhost:5000/client/'+this.id\r\n        ).then((response) => {\r\n          this.state.id = response.data.clientID;\r\n          this.state.name = response.data.clientName;\r\n          this.state.email = response.data.clientEmail;\r\n          this.state.password = response.data.clientPassword;\r\n          this.state.phone = response.data.clientPhone;\r\n        })\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async editClient() {\r\n      this.v$.$validate()\r\n      if (this.v$.$error) {\r\n        return\r\n      } \r\n      try {\r\n        await axios.put('http://localhost:5000/client/'+this.id,\r\n          {\r\n            name: this.state.name,\r\n            email: this.state.email,\r\n            password: this.state.password,\r\n            phone: this.state.phone,\r\n          }\r\n        );\r\n        this.sAlert(\"success\",\"Yeay..\",\"Editing Client Success\");\r\n        this.close();\r\n      } catch (err) {\r\n        console.log(err);\r\n        this.sAlert(\"error\",\"Oops..\",\"Editing Client Failed\");\r\n      }\r\n    },\r\n    sAlert: function(ico, tit, txt) {\r\n      Swal.fire({\r\n        icon: ico,\r\n        title: tit,\r\n        text: txt,\r\n        showConfirmButton: false,\r\n        timer: 1500\r\n      });\r\n    },\r\n    close() {\r\n      this.$emit('close');\r\n      this.$router.go();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n  .modal-backdrop {\r\n    position: fixed;\r\n    top: 0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.3);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .modal {\r\n    background: #FFFFFF;\r\n    overflow-x: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    border-radius: 5px;\r\n  }\r\n\r\n  .modal-header,\r\n  .modal-footer {\r\n    padding: 20px;\r\n    display: flex;\r\n  }\r\n\r\n  .modal-header {\r\n    position: relative;\r\n    border-bottom: 1px solid #eeeeee;\r\n    color: #4AAE9B;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .modal-footer {\r\n    border-top: 1px solid #eeeeee;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .modal-body {\r\n    position: relative;\r\n    width: 500px;\r\n    padding: 20px 20px;\r\n  }\r\n\r\n  .btn-close {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    border: none;\r\n    font-size: 20px;\r\n    padding: 20px 20px;\r\n    cursor: pointer;\r\n    font-weight: bold;\r\n    color: #4AAE9B;\r\n    background: transparent;\r\n  }\r\n\r\n  .modal-fade-enter,\r\n  .modal-fade-leave-to {\r\n    opacity: 0;\r\n  }\r\n\r\n  .modal-fade-enter-active,\r\n  .modal-fade-leave-active {\r\n    transition: opacity .5s ease;\r\n  }\r\n</style>"]}]}